{
  "name": "chaosdanmutool",
  "productName": "Chaos Danmu Tool",
  "version": "0.10.0",
  "description": "A tool for process/view Danmu on bilibili",
  "author": {
    "name": "BillGoldenWater",
    "email": "2439577268@qq.com"
  },
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "./node_modules/.bin/vite",
    "update_share": "yarn upgrade chaos_danmu_tool_share && vite build --force",
    "build": "./node_modules/.bin/tsc && vite build",
    "preview": "./node_modules/.bin/vite preview",
    "tauri": "cargo tauri"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "prettier",
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off"
    }
  },
  "dependencies": {
    "@iconscout/react-unicons": "2.0.2",
    "@popperjs/core": "2.11.7",
    "@tauri-apps/api": "1.3.0",
    "chaos_danmu_tool_share": "file:../share/pkg",
    "color": "4.2.3",
    "dot-prop": "8.0.0",
    "framer-motion": "10.12.10",
    "html2canvas": "1.4.1",
    "immutable": "^4.3.0",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-popper": "2.3.0",
    "react-use": "^17.4.0",
    "styled-components": "6.0.0-rc.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/color": "3.0.3",
    "@types/node": "20.1.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/semver": "7.5.0",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vitejs/plugin-react-swc": "3.3.1",
    "babel-plugin-styled-components": "2.1.1",
    "csstype": "3.1.2",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "internal-ip": "7.0.0",
    "prettier": "2.8.8",
    "react-devtools": "4.27.7",
    "semver": "7.5.0",
    "toml-patch": "0.2.3",
    "tslib": "2.5.0",
    "type-fest": "3.10.0",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vite-plugin-top-level-await": "^1.3.0",
    "vite-plugin-wasm": "^3.2.2"
  }
}
