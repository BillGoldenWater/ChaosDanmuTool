{
  "name": "chaosdanmutool",
  "productName": "Chaos Danmu Tool",
  "version": "0.10.0",
  "description": "A tool for process/view Danmu on bilibili",
  "author": {
    "name": "BillGoldenWater",
    "email": "2439577268@qq.com"
  },
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "(react-devtools&) && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "cargo tauri"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
